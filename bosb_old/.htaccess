Options +FollowSymlinks
RewriteEngine on
RewriteBase /bosb/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([a-z]+)/([_a-zA-Z]+)$ /bosb/htdocs_$1/$2.php?id=$2 [L]
RewriteRule ^([a-z]+)/([_a-zA-Z]+)/([_a-zA-Z]+)$ /bosb/htdocs_$1/$3.php?id=$2&&subid=$3 [L]
RewriteRule ^([a-z]+)/([_a-zA-Z]+)/([_a-zA-Z]+)/([_a-zA-Z0-9]+)$ /bosb/htdocs_$1/$4.php?id=$2&&subid=$3&&subsubid=$4[L]